const %(LanguageType)s
CCM_%(Object)s_impl::%(Identifier)s() const
    throw(LocalComponents::CCMException)
{
    DEBUGNL(" CCM_%(Object)s_impl->%(Identifier)s()");
    return %(Identifier)s_;
}

void
CCM_%(Object)s_impl::%(Identifier)s(const %(LanguageType)s value)
    throw(LocalComponents::CCMException)
{
    DEBUGNL(" CCM_%(Object)s_impl->%(Identifier)s(value)");
    %(Identifier)s_ = value;
}



