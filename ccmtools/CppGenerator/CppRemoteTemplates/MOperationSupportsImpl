%(CORBAType)s (ComponentType)s_impl::%(Identifier)s (%(MParameterDefCORBA)s) %(MExceptionDef)s;
{
  DEBUGNL(" (ComponentType)s_impl::%(Identifier)s()");

  // convert parameter(s)
  double parameter_eur;  //!!!!!!!!
  parameter_eur = CCM::CORBA_Double_to_double(eur); //!!!!!!!!!

  // call local method
  double result; //!!!!!
  result = local_component->%(Identifier)s(parameter_eur); //!!!!!!

  // convert result
  return CCM::double_to_CORBA_Double(result); //!!!!!!!!
}


