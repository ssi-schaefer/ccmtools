/**
 * Test case: remote facet module exception 
 *
 */

module world {
  module europe {
    module austria {
      
      struct ErrorInfo
      {
	long code;
	string message;
      };
      
      typedef sequence<ErrorInfo> ErrorInfoList;
      
      exception Error
      {
	ErrorInfoList info;
      };
      
      exception SuperError{};
      exception FatalError{};
      
      interface Console {
	long println(in string msg)
	  raises (Error, SuperError, FatalError);
      };
      
      component Test
      {
	provides Console console;
      };
      
      home TestHome manages Test
      {
      };

    }; // end of module austria
  }; // end of module europe
}; // end of module world
