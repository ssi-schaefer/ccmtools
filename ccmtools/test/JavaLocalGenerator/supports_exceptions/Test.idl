/**
 * Test case: supports exception 
 *
 */

module world 
{
  module europe 
  {
    module austria 
    {      
      struct ErrorInfo
      {
	long code;
	string message;
      };
      
      typedef sequence<ErrorInfo> ErrorInfoList;
      
      exception ErrorException
      {
	ErrorInfoList info;
      };
      
      exception SuperError{};

      exception FatalError
      {
	string what;
      };
      
     
      interface IFace 
      {
	long foo(in string msg)
	  raises (ErrorException, SuperError, FatalError);
      };
      

      component Test
        supports IFace
      {
      };
      
      home TestHome manages Test
      {
      };

    }; // end of module austria
  }; // end of module europe
}; // end of module world
