# Variante 1
ccmtools-generate c++local -a -o CarRental *.idl
ccmtools-generate idl3mirror -o CarRental/idl3mirror *.idl
ccmtools-generate c++local -a -o CarRental -ICarRental/idl3mirror CarRental/idl3mirror/*.idl
ccmtools-generate c++local-test -o CarRental *.idl

confix.py --packageroot=`pwd`/CarRental --bootstrap --configure --make --targets="check"


