package GLOBAL

context Benchmark
   inv i1: self.attr_l >= 0 and attr_l < 1000000
   inv i2: self.attr_s.size >= 0 and attr_s.size < 1000000
   inv i3: self.attr_ll->forAll(i| i >= 0 and i < 1000000)

context Benchmark::f_in1(l1:Integer)
   pre p1: l1 > 1 and l1 < 10

context Benchmark::f_in2(s1:String)
   pre p2: s1.size() >= 0 and s1.size() < 2000
    		
context Benchmark::f_in3(ll1:Sequence(Integer))
   pre p3: ll1->size() > 200 and ll1->size() < 1000

endpackage

