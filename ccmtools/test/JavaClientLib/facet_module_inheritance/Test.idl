/***
 * facet_module_inheritance test case
 *
 * This test case shows the usage of inheritance with IDL interfaces that are
 * used as facet type.
 ***/

module europe
{
  interface SuperType1
  {
    //    attribute long attr1;
    long op1(in string str);
  };
};


module america
{
  interface SuperType2
  {
    //    attribute long attr2;
    long op2(in string str);
  };

  interface SubType 
    : europe::SuperType1, SuperType2 
  {
    //    attribute long attr3;
    long op3(in string str);
  };
};


module world 
{
  component Test
  {
    provides america::SubType iface;
  };

  home TestHome manages Test
  {
  };
};
