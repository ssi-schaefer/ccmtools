::%(IdlProvidesType)s_ptr %(ComponentType)s_impl::provide_%(Identifier)s()
{
    LDEBUGNL(CCM_REMOTE, " %(ComponentType)s_impl::provide_%(Identifier)s()");
    if (%(Identifier)s_facet_adapter == 0) {
        %(Identifier)s_facet_adapter = new %(Identifier)sFacetAdapter(local_adapter);
        CORBA::Object_var _facet_ref = 
            ccm_container->activate_facet(this, "%(Identifier)s",
	    NULL/* _instance_for_facet_calc*/,
	    %(Identifier)s_facet_adapter);
        reference_for_facet_%(Identifier)s = ::%(IdlProvidesType)s::_narrow(_facet_ref);
    }
    return ::%(IdlProvidesType)s::_duplicate(reference_for_facet_%(Identifier)s);
}