%(CORBAType)s
%(Object)sFacetAdapter::%(Identifier)s() 
    throw(CORBA::SystemException)
{
    LDEBUGNL(CCM_REMOTE, " %(Object)sFacetAdapter->%(Identifier)s()");
    %(LanguageType)s result;	
    try {	
        result = local_adapter->%(Identifier)s();		
    }
    catch(...) {
        throw CORBA::SystemException();
    }	
    %(CORBAType)s return_value;
	CCM_Remote::convertToCorba(result, return_value);		
	return return_value;	
}

void
%(Object)sFacetAdapter::%(Identifier)s(const %(CORBAType)s value)  
    throw(CORBA::SystemException)
{
    LDEBUGNL(CCM_REMOTE, " %(Object)sFacetAdapter->%(Identifier)s(value)");
    %(LanguageType)s local_value;
    try {	
        CCM_Remote::convertFromCorba(value, local_value);
        local_adapter->%(Identifier)s(local_value);
    }
    catch(...) {
        throw CORBA::SystemException();
    }	
}
