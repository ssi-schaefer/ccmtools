

%(CORBAType)s %(ComponentType)s_impl::%(Identifier)s()
{
    LDEBUGNL(CCM_REMOTE, " %(ComponentType)s_impl::%(Identifier)s()");
    %(LanguageType)s result;
    result = local_adapter->%(Identifier)s();
    %(CORBAType)s return_value;
    CCM_Remote::convertToCorba(result, return_value);		
    return return_value;	
}

void %(ComponentType)s_impl::%(Identifier)s(const %(CORBAType)s value)
{
    LDEBUGNL(CCM_REMOTE, " %(ComponentType)s_impl::%(Identifier)s(value)");
    %(LanguageType)s parameter_value;
    CCM_Remote::convertFromCorba(value, parameter_value);	
    local_adapter->%(Identifier)s(parameter_value);
}
