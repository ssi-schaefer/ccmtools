

%(CORBAType)s %(ComponentType)s_impl::%(Identifier)s()
{
  DEBUGNL(" %(ComponentType)s_impl::%(Identifier)s() <get>");

  %(LanguageType)s result;
  result = local_adapter->%(Identifier)s();
  return CCM::%(MAttributeDefConvertResultType)s(result);
}

void %(ComponentType)s_impl::%(Identifier)s(const %(CORBAType)s value)
{
  DEBUGNL(" %(ComponentType)s_impl::%(Identifier)s() <set>");

  %(LanguageType)s local_value;
  local_value = CCM::%(MAttributeDefConvertParameter)s(value);
  local_adapter->%(Identifier)s(local_value);
}
