{
    %(ComponentType)s_%(Identifier)s_impl* facet = 
        new %(ComponentType)s_%(Identifier)s_impl(this);
    %(Identifier)s_ = dynamic_cast< %(CCMProvidesType)s* >(facet);
    return %(Identifier)s_;
}


