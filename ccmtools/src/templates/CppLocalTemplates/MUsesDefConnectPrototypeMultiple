    // Equivalent operations for multiple receptacle %(Identifier)s
    ::Components::ccm::local::Cookie 
    connect_%(Identifier)s(wamas::platform::utils::SmartPtr< %(UsesType)s> connection)
        throw(::Components::ccm::local::ExceededConnectionLimit, 
              ::Components::ccm::local::InvalidConnection);

    wamas::platform::utils::SmartPtr< %(UsesType)s> 
    disconnect_%(Identifier)s(::Components::ccm::local::Cookie ck)
        throw(::Components::ccm::local::InvalidConnection);


