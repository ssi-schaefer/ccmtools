    // Equivalent operations for multiple receptacle %(Identifier)s
    ::Components::Cookie 
    connect_%(Identifier)s(%(UsesType)s::SmartPtr connection)
        throw(::Components::ExceededConnectionLimit, ::Components::InvalidConnection);

    %(UsesType)s::SmartPtr 
    disconnect_%(Identifier)s(::Components::Cookie ck)
        throw(::Components::InvalidConnection);


