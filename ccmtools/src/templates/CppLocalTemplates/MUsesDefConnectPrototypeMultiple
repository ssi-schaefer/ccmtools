    // Equivalent operations for multiple receptacle %(Identifier)s
    ::ccm::local::Components::Cookie 
    connect_%(Identifier)s(WX::Utils::SmartPtr< %(UsesType)s> connection)
        throw(::ccm::local::Components::ExceededConnectionLimit, 
              ::ccm::local::Components::InvalidConnection);

    WX::Utils::SmartPtr< %(UsesType)s> 
    disconnect_%(Identifier)s(::ccm::local::Components::Cookie ck)
        throw(::ccm::local::Components::InvalidConnection);


