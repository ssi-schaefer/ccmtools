const %(LanguageType)s
FacetAdapter%(Object)s::%(Identifier)s() const 
    throw(::Components::ccm::local::CCMException)
{
    LDEBUGNL(CCM_LOCAL, " FacetAdapter%(Object)s::%(Identifier)s() = " 
             << ccmDebug(facet->%(Identifier)s()));
    if(!ValidConnection) 
        throw ::Components::ccm::local::InvalidConnection();
    return facet->%(Identifier)s();
}

void
FacetAdapter%(Object)s::%(Identifier)s(const %(LanguageType)s value)  
    throw(::Components::ccm::local::CCMException)
{
    LDEBUGNL(CCM_LOCAL, " FacetAdapter%(Object)s::%(Identifier)s(" 
             << ccmDebug(value) << ")");
    if(!ValidConnection) 
        throw ::Components::ccm::local::InvalidConnection();
    facet->%(Identifier)s(value);
}


