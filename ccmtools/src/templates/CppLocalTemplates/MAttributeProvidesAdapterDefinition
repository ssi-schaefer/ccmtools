const %(LanguageType)s
%(Object)sFacetAdapter::%(Identifier)s() const 
    throw(LocalComponents::CCMException)
{
    LDEBUGNL(CCM_LOCAL, " %(Object)sFacetAdapter::%(Identifier)s() = " 
             << ccmDebug(facet->%(Identifier)s()));
    if(!ValidConnection) 
        throw LocalComponents::InvalidConnection();
    return facet->%(Identifier)s();
}

void
%(Object)sFacetAdapter::%(Identifier)s(const %(LanguageType)s value)  
    throw(LocalComponents::CCMException)
{
    LDEBUGNL(CCM_LOCAL, " %(Object)sFacetAdapter::%(Identifier)s(" 
             << ccmDebug(value) << ")");
    if(!ValidConnection) 
        throw LocalComponents::InvalidConnection();
    facet->%(Identifier)s(value);
}


