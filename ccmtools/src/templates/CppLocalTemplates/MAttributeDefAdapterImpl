const %(LanguageType)s
%(InterfaceIdentifier)sAdapter::%(Identifier)s() const 
    throw(::ccm::local::Components::CCMException)
{
    LDEBUGNL(CCM_LOCAL, " %(InterfaceIdentifier)sAdapter::%(Identifier)s() = " 
             << ::%(DebugNamespace)s::ccmDebug(facet_->%(Identifier)s()));
    if(!validConnection()) 
        throw ::ccm::local::Components::InvalidConnection();
    return facet_->%(Identifier)s();
}

void
%(InterfaceIdentifier)sAdapter::%(Identifier)s(const %(LanguageType)s value)  
    throw(::ccm::local::Components::CCMException)
{
    LDEBUGNL(CCM_LOCAL, " %(InterfaceIdentifier)sAdapter::%(Identifier)s(" 
             << ::%(DebugNamespace)s::ccmDebug(value) << ")");
    if(!validConnection()) 
        throw ::ccm::local::Components::InvalidConnection();
    facet_->%(Identifier)s(value);
}


