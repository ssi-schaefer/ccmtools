    // Equivalent operations for single receptacle %(Identifier)s

    void connect_%(Identifier)s(wamas::platform::utils::SmartPtr<%(UsesType)s> i)
        throw(::Components::AlreadyConnected, ::Components::InvalidConnection);

    wamas::platform::utils::SmartPtr<%(UsesType)s> disconnect_%(Identifier)s()
        throw(::Components::NoConnection);


