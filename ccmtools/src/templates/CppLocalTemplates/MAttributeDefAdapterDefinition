const %(LanguageType)s
%(ComponentType)s::%(Identifier)s() const 
    throw(LocalComponents::CCMException)
{
    LDEBUGNL(CCM_LOCAL," %(ComponentType)s::%(Identifier)s() = "
             <<  %(DebugNamespace)sccmDebug(local_component->%(Identifier)s()));
    if(!ValidConnection) 
        throw LocalComponents::InvalidConnection();
    return local_component->%(Identifier)s();
}

void
%(ComponentType)s::%(Identifier)s(const %(LanguageType)s value)
    throw(LocalComponents::CCMException)
{
    LDEBUGNL(CCM_LOCAL," %(ComponentType)s::%(Identifier)s(" 
             << %(DebugNamespace)sccmDebug(value) << ")");
    if(!ValidConnection) 
        throw LocalComponents::InvalidConnection();
    local_component->%(Identifier)s(value);
}

