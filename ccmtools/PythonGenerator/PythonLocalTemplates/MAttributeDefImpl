    def %(Identifier)s(self):
        '''
        @rtype: %(LanguageType)s
        '''

        CCM_Utils.Debug.DEBUGNL(' %(ComponentType)s->%(Identifier)s - get semantic')
        if not self.__ValidConnection: raise localComponents.InvalidConnection()
        return self.__local_component.%(Identifier)s()

    def %(Identifier)s(self, value):
        '''
        @type value: %(LanguageType)s
        @rtype: None
        '''

        CCM_Utils.Debug.DEBUGNL(' %(ComponentType)s->%(Identifier)s - set semantic')
        if not self.__ValidConnection: raise localComponents.InvalidConnection()
        self.__local_component.%(Identifier)s(value)


